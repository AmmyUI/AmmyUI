using System;

Window "ListView" {
  ListView {
      View: GridView {
          GridViewColumn { Header: "Date" }
          GridViewColumn { 
              Header: "Day of Week",
              DisplayMemberBinding: bind DayOfWeek 
          }
          GridViewColumn { 
              Header: "Year"
              DisplayMemberBinding: bind Year
          }
      }
  
      DateTime { "1/1/2012" }
      DateTime { "1/2/2012" }
      DateTime { "1/3/2012" }
  }
}